{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "optional_permissions": [
    "topSites"
  ],
  "host_permissions": [
    "*://web.whatsapp.com/*",
    "https://*/*",
    "http://*/*"
  ],
  "permissions": ["activeTab", "tabs", "scripting"]
}